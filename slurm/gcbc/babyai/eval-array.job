#!/bin/bash

#SBATCH --partition=gpu
#SBATCH --gres=gpu:1
#SBATCH --job-name=GCBC-E
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=18
#SBATCH --time=02:00:00
#SBATCH --mem-per-gpu=120G
#SBATCH --array=1-8%8
#SBATCH --output=slurm/outputs/gcbc_eval_array_%A_%a.out

source "./slurm/.secrets"

module purge
module load 2022
module load Anaconda3/2022.05

source activate nlgoals

HPARAMS_FILE="slurm/gcbc/babyai/eval-array.txt"

srun python src/nlgoals/run/babyai/eval-gcbc.py \
  --num_rollouts 1000 \
  --num_rollout_steps 50 \
  --verbose true \
  --model_variant BABYAI \
  --clipt_checkpoint checkpoints/babyai/cclipt/last.ckpt \
  $(head -$SLURM_ARRAY_TASK_ID $HPARAMS_FILE | tail -1)
