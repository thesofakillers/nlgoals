# make a copy of this file named .secrets and edit it so that contains wandb key
export WANDB_API_KEY=YOUR_API_KEY
# optionally you can set the cache dir to something that can handle more storage
export WANDB_CACHE_DIR=$HOME/.cache/wandb
# optionally can set the sweep id if running a sweep (including entity and project)
export WANDB_SWEEP_ID=ENTITY/PROJECT/YOUR_SWEEP_ID
